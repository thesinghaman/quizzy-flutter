# Flutter specific
.flutter-plugins
.flutter-plugins-dependencies

# Visual Studio Code specific
.vscode/           # Visual Studio Code settings and workspace files
*.code-workspace   # Visual Studio Code workspace file

# macOS specific files to ignore
.DS_Store

