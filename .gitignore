*.iml
.idea/             # Android Studio settings
.gradle/           # Gradle build directory
/local.properties # Android SDK path
/.idea/workspace.xml
/.idea/libraries
/.idea/caches
/.idea/modules.xml
/.gradle/
/captures/

# iOS
Pods/
Podfile.lock
